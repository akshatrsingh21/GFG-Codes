class Solution {
public:
    long long subarraySum(vector<int>& arr) {
        int n = arr.size();
        long long totalSum = 0;

        for (int i = 0; i < n; i++) {
            long long contribution = (long long)(i + 1) * (n - i);
            totalSum += arr[i] * contribution;
        }

        return totalSum;
    }
};
